{
  "name": "fcl-web-app",
  "version": "1.0.0",
  "license": "GPL v3",
  "scripts": {
    "ng": "ng",
    "serverlessstart": "ng serve --configuration=serverless",
    "start": "ng serve --proxy-config proxy.config.json",
    "serve": "ng serve --proxy-config proxy.config.json --watch",
    "serve:fcldev": "ng serve --proxy-config proxy.config.fcldev.json --watch",
    "serve:local": "ng serve",
    "build:qa": "npm run clean && npm run lint && ng build --configuration=qa --output-path \"public\"",
    "build:qa_wo_lint": "npm run clean && ng build --configuration=qa --output-path \"public\"",
    "build:prod": "npm run clean && npm run lint && ng build --configuration=production --output-path \"public\"",
    "dependency-check:prod": "npx npm-auditor-ci --threshold low --ignoreDev",
    "dependency-check:qa": "npx npm-auditor-ci --threshold low",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "test:update-snapshot": "jest --updateSnapshot",
    "lint": "npm run lint:code && npm run lint:styles",
    "lint:code": "tslint --project tsconfig.json --fix -c tslint.json \"src/**/*.ts\"",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "e2e": "ng e2e",
    "gh-pages": "ng build --configuration=production --bh /fcl-client/",
    "cloud9": "ng serve --host $IP --port $PORT",
    "clean": "rimraf public/*"
  },
  "private": true,
  "dependencies": {
